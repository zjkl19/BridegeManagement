@model ExcelImportBridgeViewModel

<h3>@ViewData["Title"]</h3>
<div class="row">
    <partial name="StatusMessagePartial" model='Model.StatusMessage'>
</div>
<div class="row">
    <div class="col-md-2 col-4">
        <a asp-action="Index" class="btn btn-info"><i class="fa fa-arrow-circle-left"></i> 返回首页</a>
    </div>
</div>
<p></p>
<hr />
<div class="row">
    <div class="col-6 col-md-5">
        <div class="alert alert-info">
            批量导入时，请仔细校对各行数据的有效性。
        </div>
        <div>
            <form enctype="multipart/form-data" method="post" asp-action="ExcelImportBridge">
                <div class="form-group">
                    <label asp-for="ExcelImport" class="control-label"></label>
                    <input asp-for="ExcelImport" type="file" class="form-control" />
                    <span asp-validation-for="ExcelImport" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <input type="submit" class="btn btn-primary" value="提交" />
                </div>
            </form>
        </div>

    </div>
</div>
<p></p>
<div>
    <input type="button" class="btn btn-secondary" name="close" value="关闭当前窗口" onclick="window.close();">
</div>